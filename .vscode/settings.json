{
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.import.gradle.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/*~": true
  },
  "C_Cpp.default.configurationProvider": "vscode-wpilib",
  "files.associations": {
    "type_traits": "cpp",
    "ratio": "cpp",
    "array": "cpp",
    "*.inc": "cpp",
    "*.tcc": "cpp",
    "random": "cpp",
    "typeinfo": "cpp",
    "new": "cpp"
  },
  "C_Cpp.intelliSenseEngineFallback": "disabled"
}
